<!DOCTYPE html>
<html lang="fa" class="light-style layout-navbar-fixed layout-menu-fixed" dir="rtl" data-theme="theme-default" data-assets-path="~/assets/" data-template="vertical-menu-template-starter">
<head>
</head>
<partial name="Partial/_AdminHeadTags.cshtml" />
@await RenderSectionAsync("Style",false)
<body>
    <!-- Layout wrapper -->
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            <!-- Menu -->
            <partial name="Partial/_AdminMenu.cshtml" />

            <!-- / Menu -->
            <!-- Layout container -->
            <div class="layout-page">
                <!-- Navbar -->
                <partial name="Partial/_AdminNavbar.cshtml" />

                <!-- / Navbar -->
                <!-- Content wrapper -->
                <div class="content-wrapper">
                    <!-- Content -->

                    <div class="container-xxl flex-grow-1 container-p-y">
                        @RenderBody()
                    </div>
                    <!-- / Content -->
                    <!-- Footer -->
                    <partial name="Partial/_AdminFooter.cshtml" />
                    
                   
                    <!-- / Footer -->

                    <div class="content-backdrop fade"></div>
                </div>
                <!-- Content wrapper -->
            </div>
            <!-- / Layout page -->
        </div>

        <!-- Overlay -->
        <div class="layout-overlay layout-menu-toggle"></div>

        <!-- Drag Target Area To SlideIn Menu On Small Screens -->
        <div class="drag-target"></div>
    </div>
   
    <partial name="Partial/_AdminScript.cshtml" />

    <!-- Page JS -->
    @await RenderSectionAsync("Script", false)

</body>
</html>